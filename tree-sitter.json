{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tree_sitter_rux",
      "camelcase": "TreeSitterRux",
      "title": "Tree Sitter Rux",
      "scope": "source.tree_sitter_rux",
      "file-types": [
        "rx"
      ],
      "injection-regex": "^tree_sitter_rux$",
      "class-name": "TreeSitterTreeSitterRux"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A Tree Sitter Parser for the Rux Programming Language",
    "authors": [
      {
        "name": "Mika",
        "email": "mika.assmus@gmail.com",
        "url": "https://github.com/mikatschuh"
      }
    ],
    "links": {
      "repository": "https://github.com/mikatschuh/tree-sitter-rux"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}